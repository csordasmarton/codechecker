{
  "name": "codechecker",
  "version": "1.0.0",
  "private": true,
  "description": "CodeChecker viewer.",
  "scripts": {
    "build:dev": "npm run thrift-api && rimraf dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run thrift-api && rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build": "npm run build:dev",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache verify && npm run rimraf -- api dist node_modules",
    "postinstall": "npm run webdriver:update",
    "server:prod": "http-server dist -c-1 --cors",
    "server:dev": "npm run thrift-api && webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "test": "karma start --single-run",
    "thrift-api": "rimraf ./api && thrift-typescript --rootDir . --sourceDir ../api  --outDir ./api",
    "tslint": "tslint -p ./tslint.json",
    "webdriver:update": "node ./node_modules/protractor/bin/webdriver-manager update"
  },
  "license": "University of Illinois/NCSA Open Source License",
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@creditkarma/thrift-typescript": "^3.0.0",
    "angular-tree-component": "^5.2.1",
    "bootstrap": "^4.1.3",
    "codemirror": "^5.32.0",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
    "jsplumb": "2.5.7",
    "ngx-popover": "^0.0.16",
    "popper.js": "^1.13.0",
    "rxjs": "^6.3.3",
    "thrift": "^0.11.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@swimlane/ngx-datatable": "^13.1.0",
    "@types/codemirror": "^0.0.50",
    "@types/jasmine": "^2.8.8",
    "@types/jsplumb": "^1.3.31",
    "@types/node": "^8.5.1",
    "@types/thrift": "^0.10.7",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "bootstrap-loader": "^3.0.1",
    "bootstrap-sass": "^3.3.7",
    "codelyzer": "^4.4.2",
    "copy-webpack-plugin": "^4.5.3",
    "css-loader": "^1.0.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.8.0",
    "jquery": "^3.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "protractor": "^5.3.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "to-string-loader": "^1.1.5",
    "tslint": "~5.11.0",
    "typescript": "^2.7.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  }
}
